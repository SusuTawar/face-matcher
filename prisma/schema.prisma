// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Matches {
  id       String   @id
  filePath String
  expires  DateTime
}

model ComplaintHistory {
  id        BigInt   @id @default(autoincrement())
  requestId String
  file      String
}
